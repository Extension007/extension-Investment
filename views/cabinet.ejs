  <main class="container">
    <section class="section">
      <h1>–õ–∏—á–Ω—ã–π –∫–∞–±–∏–Ω–µ—Ç</h1>
      
      <!-- –í–∫–ª–∞–¥–∫–∏ -->
      <div class="tabs">
        <button class="tab active" data-tab="products">–ú–æ–∏ —Ç–æ–≤–∞—Ä—ã</button>
        <button class="tab" data-tab="services">–ú–æ–∏ —É—Å–ª—É–≥–∏</button>
        <button class="tab" data-tab="banners">–ú–æ–∏ –±–∞–Ω–Ω–µ—Ä—ã</button>
        <button class="tab" data-tab="create">–°–æ–∑–¥–∞—Ç—å –∫–∞—Ä—Ç–æ—á–∫—É</button>
      </div>

      <!-- –í–∫–ª–∞–¥–∫–∞: –ú–æ–∏ —Ç–æ–≤–∞—Ä—ã -->
      <div class="tab-content active" id="tab-products">
        <h2>–ú–æ–∏ —Ç–æ–≤–∞—Ä—ã</h2>
        <% if (products && products.length > 0) { %>
