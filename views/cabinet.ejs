      <!-- –í–∫–ª–∞–¥–∫–∞: –ú–æ–∏ —É—Å–ª—É–≥–∏ -->
      <div class="tab-content" id="tab-services">
        <h2>–ú–æ–∏ —É—Å–ª—É–≥–∏</h2>
        <% if (services && services.length > 0) { %>
          <div class="product-grid">
            <% services.forEach(service => { %>
              <%- include('partials/card', { service, itemType: 'service', mode: 'user', user, socket_io_available }) %>
            <% }) %>
